{
    "name": "samuelbie/helpers",
    "description": "Just some helpers for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Bi√©",
            "email": "samuelbie@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.5 || ^7.0",
        "nesbot/carbon": "^2.17"
    },
    "autoload": {
        "files": [
            "src/functions/helpers.php"
        ],
        "psr-4": {
            "Samuelbie\\Helpers\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Samuelbie\\Helpers\\HelpersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
