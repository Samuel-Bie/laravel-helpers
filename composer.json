{
    "name": "samuelbie/helpers",
    "description": "Just some helpers for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Bi√©",
            "email": "samuelbie@outlook.com"
        }
